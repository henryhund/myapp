<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tagline, input_html: { rows: 5, cols: 10}  %>
    <%= f.input :bio, label: 'About You', input_html: { rows: 5, cols: 10} %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>


<% end %>
